{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "env": {
    "VITE_API_URL": {
      "description": "API endpoint URL"
    },
    "VITE_ENABLE_ANALYTICS": {
      "value": "true"
    }
  },
  "functions": {
    "api/**": {
      "runtime": "nodejs18.x"
    }
  }
}
